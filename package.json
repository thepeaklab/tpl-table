    "postcss-sorting": "1.6.1",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "stylefmt": "4.0.0",
    "stylelint": "6.7.1",
    "stylelint-config-standard": "9.0.0",
    "tslint": "3.11.0",
    "tslint-loader": "2.1.4",
    "typescript": "1.8.10",
