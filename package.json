    "ng-annotate": "1.2.1",
    "ng-annotate-loader": "0.1.0",
    "null-loader": "0.1.1",
    "phantomjs-prebuilt": "2.1.6",
    "pngsmith": "0.1.3",
    "postcss": "5.0.21",
    "postcss-assets-rebase": "0.4.0",
    "postcss-cli": "2.5.2",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.9.1",
    "postcss-mixins": "5.0.0",
