    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "scripts": {
    "init": "npm i",
    "typings": "typings",
    "postinstall": "typings install",
    "clean": "rimraf .tmp dist",
    "lint": "tslint src/**/*.ts",
    "test": "karma start config/karma.conf.js",
    "css": "postcss -c postcss.config.json",
    "prefonts": "mkdir -p dist/fonts",
    "fonts": "cp -R src/fonts/ dist/fonts/",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "start": "npm run replace:dev && webpack-dev-server --history-api-fallback --inline --progress --host 0.0.0.0 --TARGET=DEV",
    "prebuild": "npm run clean",
    "build": "npm run lint && (npm run css & npm run fonts & tsc -d)",
    "replace:dev": "cp config/environments/dev.json src/bootstrap/env.json"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "core-js": "2.4.0",
    "lodash": "4.6.1",
    "ng2-translate": "2.2.2",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.9",
    "zone.js": "0.6.12"
  },
  "main": "dist/index.js",
  "style": "dist/tpl-table.css",
  "typings": "./dist/index.d.ts"
